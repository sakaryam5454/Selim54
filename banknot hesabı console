using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication2
{
    class Program
    {

        static void Main(string[] args)
        {
            long para, kalan;
            long ikiyuz, yuz, elli, yirmi, on, bes;

            top:
            Console.WriteLine("Çekilecek miktarı giriniz. \n");
            para = Convert.ToInt64(Console.ReadLine());
            if (para < 0) {
                Console.WriteLine("Girdiğiniz para miktarı negatif olamaz. \n");
                goto top;
            }
            
            if (para % 5 != 0) // Paranın 5 'in katı olup olmadığı kontrolü
            {
                Console.WriteLine("Girilen miktar 5 ve 5 'in katı olmalıdır.\n");
                goto top;
            }
            ikiyuz = para / 200; // 200 'lük para miktarı
            kalan = para % 200; // 200 'lük para miktarından kalan

            yuz = kalan1 / 100; // 100 'lük para miktarı
            kalan = kalan % 100; // 100 'lük para miktarından kalan

            elli = kalan2 / 50; // 50 'lik para miktarı
            kalan = kalan % 50; // 50 'lik para miktarından kalan

            yirmi = kalan3 / 20; // 20 'lik para miktarı
            kalan = kalan % 20; // 20 'lik para miktarından kalan

            on = kalan4 / 10; // 10 'luk para miktarı
            kalan = kalan % 10; // 10 'luk para miktarından kalan

            bes = kalan5 / 5; // 5 'lik para miktarı

            else
            {
                Console.WriteLine("200 'lük banknot: " + ikiyuz + "\n100'lük banknot:" + yuz + "\n50'lik banknot:" + elli +"\n20'lik banknot:" + yirmi + "\n10'luk banknot:" + on +  "\n5'lik banknot:" + bes);
            }

            Console.ReadLine();
        }
    }
}
